
<% for (let i = 0; i < user.length; i++) { %>
    <% for (let i = 0; i < goal.length; i++) { %>
        <!-- Modal -->
<div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="staticBackdropLabel">Completed Goal plan</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
            Are you sure your goal has been achieved? 
        </div>
        <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">No</button>
        <a href="/goals/complete/<%= user[i].id %>/<%= goal[i].id %>" class="btn btn-primary rounded-pill px-3 mx-3 d-none d-lg-block" >Yes</a>
        </div>
      </div>
    </div>
  </div>
<% }} %>